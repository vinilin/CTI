#ifndef ${TYPE_PREFIX_UPPER}_INTTYPES_H
#define ${TYPE_PREFIX_UPPER}_INTTYPES_H

#cmakedefine ${TYPE_PREFIX_UPPER}_HAVE_INTTYPES_H

